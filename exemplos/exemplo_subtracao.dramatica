// Exemplo com subtração em DRAMATICA
// Demonstra operação de subtração conforme requisitos

scene Subtracao:
    character Operador:
        memory:
            num1: number;
            num2: number;
            diferenca: number;
        FIM_MEMORY

    LEIA num1;
    LEIA num2;
    diferenca = num1 - num2;
    Operador says diferenca;
FIM_CENA

