// Exemplo com subtração em DRAMATICA
// Demonstra operação de subtração conforme requisitos

CENA Subtracao:
    PERSONAGEM Operador:
        MEMORIA:
            num1: INT;
            num2: INT;
            diferenca: INT;
        FIM_MEMORIA

    LEIA num1;
    LEIA num2;
    diferenca = num1 - num2;
    Operador DIZ diferenca;
FIM_CENA

